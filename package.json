    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.58.1",
    "recharts": "^3.0.0",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.5"
