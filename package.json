    "@radix-ui/react-toast": "^1.2.14",
    "@sendgrid/mail": "^8.1.5",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
