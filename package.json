{
  "name": "inventory-app-rebuild",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:staging": "dotenv -e .env.staging -- next dev",
    "dev:prod-readonly": "dotenv -e .env.production-readonly -- next dev",
    "build": "next build",
    "build:prod": "NODE_ENV=production next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "env:check": "node scripts/check-env.js",
    "env:validate": "node scripts/validate-env.js",
    "db:pull": "prisma db pull",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio --readonly",
    "db:studio:write": "prisma studio",
    "seed:location": "npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed-location.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
